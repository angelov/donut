{
    "name": "angelov/donut",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Angelov\\Donut\\": "src/Donut",
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Angelov\\Donut\\Tests\\Api\\": "tests/Api",
            "Angelov\\Donut\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "symfony/symfony": "^3.4",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "doctrine/migrations": "^1.5",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "snc/redis-bundle": "^2.0",
        "predis/predis": "^1.1",
        "neo4j/neo4j-bundle": "^0.4",
        "simple-bus/symfony-bridge": "^4.1",
        "graphaware/reco4php": "^2.0",
        "simple-bus/asynchronous-bundle": "^2.3",
        "simple-bus/jms-serializer-bundle-bridge": "^2.0",
        "simple-bus/rabbitmq-bundle-bridge": "^3.1",
        "symfony/web-server-bundle": "^3.3",
        "friendsofsymfony/rest-bundle": "^2.2",
        "friendsofsymfony/oauth-server-bundle": "^1.5"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "hautelook/alice-bundle": "^1.4",
        "doctrine/data-fixtures": "^1.2",
        "vimeo/psalm": "^0.3",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/symfony2-extension": "^2.1",
        "behat/mink-goutte-driver": "^1.2",
        "friends-of-behat/context-service-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.0",
        "friends-of-behat/cross-container-extension": "^1.0",
        "friends-of-behat/variadic-extension": "^1.0",
        "webmozart/assert": "^1.2",
        "phpspec/phpspec": "^4.0",
        "phpunit/phpunit": "^6.0",
        "dama/doctrine-test-bundle": "^4.0",
        "fzaninotto/faker": "^1.6",
        "lakion/api-test-case": "^2.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
