{
    "name": "angelov/donut",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Angelov\\Donut\\": "src/Donut",
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Angelov\\Donut\\Tests\\Api\\": "tests/Api",
            "Angelov\\Donut\\Tests\\": "tests/",
            "spec\\Angelov\\": "spec/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "behat/mink-goutte-driver": "^1.2",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/migrations": "^3.1",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/oauth-server-bundle": "^1.6.2",
        "friendsofsymfony/rest-bundle": "^3.0",
        "graphaware/reco4php": "dev-symfony-4",
        "neo4j/neo4j-bundle": "^0.4.2",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^6.1",
        "simple-bus/asynchronous-bundle": "^3.0",
        "simple-bus/jms-serializer-bundle-bridge": "^3.0",
        "simple-bus/rabbitmq-bundle-bridge": "^4.0",
        "simple-bus/symfony-bridge": "^5.1",
        "snc/redis-bundle": "^3.4",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.10",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/security-csrf": "^4.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/web-server-bundle": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "dama/doctrine-test-bundle": "^6.5.0",
        "dms/phpunit-arraysubset-asserts": "^0.3.0",
        "doctrine/data-fixtures": "^1.2",
        "friends-of-behat/mink": "^1.9",
        "friends-of-behat/mink-browserkit-driver": "^1.5",
        "friends-of-behat/mink-extension": "^2.5",
        "friends-of-behat/service-container-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.2",
        "friends-of-behat/variadic-extension": "^1.4",
        "fakerphp/faker": "^1.14",
        "lchrusciel/api-test-case": "^5.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "phpspec/phpspec": "^7.0",
        "phpstan/phpstan": "^0.12.88",
        "phpstan/phpstan-doctrine": "^0.12.37",
        "phpstan/phpstan-phpunit": "^0.12.19",
        "phpstan/phpstan-symfony": "^0.12.33",
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^4.1",
        "symfony/phpunit-bridge": "^4.1",
        "webmozart/assert": "^1.2"
    },

    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/angelov/reco4php"
        },
        {
            "type": "git",
            "url": "https://github.com/angelov/CrossContainerExtension"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
