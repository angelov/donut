default:

    extensions:
        Behat\Symfony2Extension: ~
        Behat\MinkExtension:
            base_url: 'http://localhost:8000'
            goutte: ~
            default_session: symfony
            sessions:
                symfony:
                    symfony2: ~

    suites:
        users:
            contexts:
                - DoctrineHookContext
                - RegistrationContext
            filters:
                tags: "@users"